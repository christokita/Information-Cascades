behav_sum <- behav_data %>%
select(-replicate) %>%
group_by(gamma) %>%
summarise_each(funs(mean(., na.rm = TRUE), sd(., na.rm = TRUE))) %>%
mutate(correct_message_95ci = qnorm(0.975)*correct_message_sd/sqrt(100),
